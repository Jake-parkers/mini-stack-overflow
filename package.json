{
  "name": "mini-stack-overflow",
  "version": "1.0.0",
  "description": "A Simple Clone of Stack Overflow with features: Login, Signup, Ask Questions, View Questions, upvote or downvote questions, Answer Question, Search (Question, Answers, Users)",
  "main": "server.js",
  "scripts": {
    "build": "tsc --project ./",
    "dev": "ts-node-dev --respawn --transpile-only --notify --rs ./src/server.ts",
    "start": "tsc && nodemon dist/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jake-parkers/mini-stack-overflow.git"
  },
  "keywords": [
    "Stack",
    "Overflow",
    "Programming",
    "Questions",
    "Answers"
  ],
  "author": "Joshua Boateng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jake-parkers/mini-stack-overflow/issues"
  },
  "homepage": "https://github.com/Jake-parkers/mini-stack-overflow#readme",
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.6.0",
    "node-notifier": "^8.0.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.60",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
